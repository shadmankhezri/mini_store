<odoo>
  <data noupdate="1">
    <record id="seq_mini_store_invoice" model="ir.sequence">
      <field name="name">Mini Store Invoice</field>
      <field name="code">mini_store.invoice</field>
      <field name="prefix">INV/</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
    </record>
  </data>



  <data noupdate="1">
    <record id="seq_mini_store_sale_order" model="ir.sequence">
      <field name="name">Mini Store Sale Order</field>
      <field name="code">mini_store.sale_order</field>
      <field name="prefix">SO/</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
    </record>
  </data>
</odoo>
